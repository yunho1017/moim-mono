<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, viewport-fit=cover"
    />
    <base href="/" />
    <link
      rel="dns-prefetch"
      href="https://s3-us-west-2.amazonaws.com/crypto-badge-static-prod/assets"
    />

    <% if (process.env.PHASE == "PRODUCTION") { %>
    <link rel="dns-prefetch" href="https://moim.co/api" />
    <link rel="dns-prefetch" href="https://ca.group-edge.net" />
    <link rel="dns-prefetch" href="https://files.moim.co" />
    <link rel="dns-prefetch" href="https://payment.moim.co" />
    <link 
      rel="preconnect" 
      href="https://moim.co/api" 
      crossorigin="anonymous" 
    />
    <link
      rel="preconnect"
      href="https://files.moim.co/files"
      crossorigin="anonymous"
    />

    <% } else if(process.env.PHASE == "STAGING") { %>
    <link rel="dns-prefetch" href="https://vingle.network/api" />
    <link rel="dns-prefetch" href="https://files.vingle.network" />
    <link rel="dns-prefetch" href="https://payment.vingle.network" />
    <link
      rel="preconnect"
      href="https://vingle.network/api"
      crossorigin="anonymous"
    />
    <link
      rel="preconnect"
      href="https://files.vingle.network/files"
      crossorigin="anonymous"
    />
    <% } else { %>
    <% }  %>

    <script src="https://apis.google.com/js/api.js"></script>
    <script src="https://developers.kakao.com/sdk/js/kakao.js"></script>
    <link href="https://vjs.zencdn.net/7.2.4/video-js.css" rel="stylesheet" />

    <% for (const file of htmlWebpackPlugin.files.css) { %>
    <link href="<%= file %>" rel="stylesheet" />
    <% } %>
  </head>

  <body>
    <div id="vingle-app"></div>
    <div id="vingle-portal"></div>
    <% for (const file of htmlWebpackPlugin.files.js) { %>
    <script src="<%= file %>"></script>
    <% } %>
  </body>
</html>
