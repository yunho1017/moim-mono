{
  "name": "moim-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "analyze": "rm -rf dist && webpack -p --profile --json --config webpack.browser.config.js | awk '{if (NR>=8) print}' > ./node_modules/.profile.json && webpack-bundle-analyzer ./node_modules/.profile.json",
    "build-prod": "npm run poeditor-pull && rm -rf dist && gulp package",
    "build-stage": "npm run poeditor-pull && rm -rf dist && rm -rf public && gulp package-stage && mv dist public",
    "deploy-to-stage": "cross-env PHASE=STAGING gulp deploy-to-stage",
    "deploy-to-production": "cross-env PHASE=PRODUCTION gulp deploy-to-production",
    "dev": "npm run poeditor-pull && env NODE_OPTIONS=--max_old_space_size=4096 NODE_OPTIONS=--max-http-header-size=100000 webpack-dev-server --config webpack.dev.config.js --content-base app/ --progress --colors --host 0.0.0.0 --https --key ./pems/lvh.key --cert ./pems/lvh.cert",
    "dev:production": "cross-env PHASE=TESTING gulp servo-build && mkdir -p ./dist/app/v1 && mv dist/*.(js|map|json) dist/app/v1/ && npx http-server ./dist -p 9000",
    "test:unit": "node --max-old-space-size=4096 $(npm bin)/jest '.*app.*' --coverage --passWithNoTests",
    "test:e2e": "node --max-old-space-size=4096 $(npm bin)/jest '__tests__/scenarios/*' --coverage --passWithNoTests",
    "test": "rm -rf output && npm run test:unit",
    "check:ts-compile": "env NODE_OPTIONS=--max_old_space_size=4096 tsc -p ./tsconfig.json --noEmit",
    "storybook": "npm run poeditor-pull && start-storybook -p 9001 -c .storybook",
    "poeditor-pull": "gulp poeditor-pull",
    "deploy-storybook": "npm run poeditor-pull && env NODE_OPTIONS=--max_old_space_size=4096 storybook-to-ghpages"
  },
  "lint-staged": {
    "{scripts,app,__{tests,mocks}__}/**/*.{ts,tsx,scss}": [
      "prettier --write",
      "git add"
    ]
  },
